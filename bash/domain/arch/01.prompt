local prompt_color=$BLDGRE
local pre_prompt
if [ "$(id -u)" == "0" ]; then
  # running as root
  pre_prompt="$BLDRED\u$BLDWHT@$TXTDEF"
elif [ "$(whoami)" == "duff" ]; then
  # running as duff
  pre_prompt="$BLDYEL\u$BLDWHT@$TXTDEF"
fi
PS1="$pre_prompt$prompt_color\h:$BLDWHT \w\$(parse_git_branch)$TXTDEF\n\$ "
