PROMPT_COLOR=$BLDGRE
if [ "$(id -u)" == "0" ]; then
  # running as root
  PROMPT_COLOR=$TXTDEF
  PRE_PROMPT="$BLDRED\u$TXTDEF@"
elif [ $(whoami)=="duff" ]; then
  # running as duff
  PROMPT_COLOR=$TXTDEF
  PRE_PROMPT="$BLDYEL\u$TXTDEF@"
fi
PS1="$PRE_PROMPT$PROMPT_COLOR\h:$BLDWHT \w\$(parseGitBranch)$PROMPT_COLOR\n\$ $TXTDEF"
