local prompt_color=$BLDBLU
local pre_prompt
if [ "$(id -u)" == "0" ]; then
  # running as root
  pre_prompt="$BLDRED\u$BLDWHT@$TXTDEF"
elif [ "$(whoami)" == "vagrant" ]; then
  # running as vagrant
  pre_prompt="$BLDGRE\u$BLDWHT@$TXTDEF"
fi
PS1="$pre_prompt$prompt_color\H:$BLDWHT \w\$(parse_git_branch)$TXTDEF\n\$ "
